{
  "name": "DMS: Sachakte suchen",
  "id": "de.muenchen.digitalwf.templates.search-file",
  "appliesTo": [
    "bpmn:CallActivity"
  ],
  "properties": [
    {
      "label": "Template",
      "type": "String",
      "editable": false,
      "value": "StreamingTemplateV02",
      "binding": {
        "type": "property",
        "name": "calledElement"
      }
    },
    {
      "label": "Dms System (Integration Name)",
      "type": "Dropdown",
      "value": "mucsDmsIntegration",
      "choices": [
        {
          "name": "mucs",
          "value": "mucsDmsIntegration"
        },
        {
          "name": "alw",
          "value": "alwDmsIntegration"
        }
      ],
      "binding": {
        "type": "camunda:in",
        "name": "app_integration_name",
        "target": "app_integration_name",
        "expression": true
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Type Header",
      "type": "String",
      "editable": false,
      "value": "searchFile",
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "name": "app_type_name",
        "target": "app_type_name"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Suche",
      "type": "String",
      "description": "untergruppe.*-sachakte-*",
      "value": "",
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "name": "searchString",
        "target": "searchString"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "User",
      "type": "String",
      "value": "",
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "name": "user",
        "target": "user"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Fachdatum Referenz",
      "type": "String",
      "value": "",
      "description": "Suche auf ein Fachdatum eingrenzen",
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "name": "reference",
        "target": "reference"
      },
      "constraints": {
        "notEmpty": false
      }
    },
    {
      "label": "Fachdatum Wert",
      "type": "String",
      "value": "",
      "description": "Wert f√ºr die Eingrenzung der Suche auf ein Fachdatum",
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "name": "value",
        "target": "value"
      },
      "constraints": {
        "notEmpty": false
      }
    },
    {
      "label": "Sachakten (Coo)",
      "description": "Out Mapping",
      "type": "array",
      "items": {
        "type": "string"
      },
      "value": "",
      "binding": {
        "type": "camunda:out",
        "source": "fileCoo"
      },
      "constraints": {
        "notEmpty": true
      }
    }
  ]
}
