{
  "name": "DMS: Sachakte erstellen",
  "id": "de.muenchen.digitalwf.templates.create-file",
  "appliesTo": [
    "bpmn:CallActivity"
  ],
  "properties": [
    {
      "label": "Template",
      "type": "String",
      "editable": false,
      "value": "StreamingTemplateV02",
      "binding": {
        "type": "property",
        "name": "calledElement"
      }
    },
    {
      "label": "Event Topic",
      "type": "String",
      "value": "",
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "name": "app_topic_name",
        "target": "app_topic_name"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Event Message",
      "type": "String",
      "editable": false,
      "value": "genericEvent",
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "name": "app_message_name",
        "target": "app_message_name"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Type Header",
      "type": "String",
      "editable": false,
      "value": "createFile",
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "name": "app_type_name",
        "target": "app_type_name"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Title",
      "type": "String",
      "value": "",
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "name": "title",
        "target": "title"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "User",
      "type": "String",
      "value": "",
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "name": "user",
        "target": "user"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Aktenplan (Coo)",
      "type": "String",
      "value": "",
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "name": "apentryCOO",
        "target": "apentryCOO"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "File (Coo)",
      "description": "Out Mapping",
      "type": "String",
      "value": "",
      "binding": {
        "type": "camunda:out",
        "source": "fileCOO"
      },
      "constraints": {
        "notEmpty": true
      }
    }
  ]
}
