{
  "name": "Schriftstuecke speichern",
  "id": "de.muenchen.digitalisierung.templates.dms.SchriftstueckeSpeichern",
  "appliesTo": [
    "bpmn:ServiceTask"
  ],
  "properties": [
    {
      "label": "Delegate Expression",
      "type": "Hidden",
      "value": "${saveSchriftstueckeDelegate}",
      "editable": false,
      "binding": {
        "type": "property",
        "name": "camunda:delegateExpression"
      }
    },
    {
      "label": "Benutzer DMS",
      "description": "Input",
      "type": "String",
      "binding": {
        "type": "camunda:inputParameter",
        "name": "dms_user_login"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Vorgang COO",
      "description": "Input",
      "type": "String",
      "binding": {
        "type": "camunda:inputParameter",
        "name": "dms_vorgang_coo"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Dokument Name",
      "description": "Input",
      "type": "String",
      "binding": {
        "type": "camunda:inputParameter",
        "name": "dms_document_name"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Dokument COO",
      "description": "Output",
      "type": "String",
      "binding": {
        "type": "camunda:outputParameter",
        "source": "${dms_document_coo}"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Schriftst√ºcke Metadata",
      "description": "Output",
      "type": "String",
      "binding": {
        "type": "camunda:outputParameter",
        "source": "${dms_save_schriftstuecke_result}"
      },
      "constraints": {
        "notEmpty": true
      }
    }
  ],
  "entriesVisible": {
    "_all": true
  }
}