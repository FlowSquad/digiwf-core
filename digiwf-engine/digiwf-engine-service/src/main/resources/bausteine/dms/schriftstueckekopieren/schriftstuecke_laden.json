        "type": "camunda:inputParameter",
        "name": "dms_user_login"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Schriftst√ºcke",
      "description": "Input",
      "type": "String",
      "binding": {
        "type": "camunda:inputParameter",
        "name": "dms_schriftstuecke"
      },
      "constraints": {
        "notEmpty": true
      }
    }
