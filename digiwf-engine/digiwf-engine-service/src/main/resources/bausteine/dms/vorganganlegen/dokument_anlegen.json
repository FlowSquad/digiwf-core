{
  "name": "Dokument anlegen",
  "id": "de.muenchen.digitalisierung.templates.dms.DokumentAnlegen",
  "appliesTo": [
    "bpmn:ServiceTask"
  ],
  "properties": [
    {
      "label": "Delegate Expression",
      "type": "Hidden",
      "value": "${createDokumentDelegate}",
      "editable": false,
      "binding": {
        "type": "property",
        "name": "camunda:delegateExpression"
      }
    },
    {
      "label": "Prozess Id",
      "description": "Input - Id des Prozesses aus dem ein Dokument erstellt werden soll",
      "type": "String",
      "binding": {
        "type": "camunda:inputParameter",
        "name": "dms_prozess_id"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Cosys Guid",
      "description": "Input",
      "type": "String",
      "binding": {
        "type": "camunda:inputParameter",
        "name": "dms_cosys_guid"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Schriftstück name",
      "description": "Input",
      "type": "String",
      "binding": {
        "type": "camunda:inputParameter",
        "name": "dms_schriftstueck_name"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Vorgang COO",
      "description": "Input",
      "type": "String",
      "binding": {
        "type": "camunda:inputParameter",
        "name": "dms_vorgang_coo"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "DMS Benutzer",
      "description": "Input",
      "type": "String",
      "binding": {
        "type": "camunda:inputParameter",
        "name": "dms_user_login"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Erstelltes Schriftstück",
      "description": "Output",
      "type": "String",
      "binding": {
        "type": "camunda:outputParameter",
        "source": "${dms_created_schriftstueck}"
      },
      "constraints": {
        "notEmpty": true
      }
    }
  ],
  "entriesVisible": {
    "_all": true
  }
}